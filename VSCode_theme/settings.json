    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "textMateRules": [
                {
                    "scope": "variable.other.readwrite.powershell",
                    "settings": {
                        "foreground": "#D3B692"
                    }
                },
                {
                    "scope": "support.function.powershell",
                    "settings": {
                        "foreground": "#72B7C0"
                    }
                },
                {
                    "scope": "string.quoted.double.powershell",
                    "settings": {
                        "foreground": "#72C09F"
                    }
                },
                {
                    "scope": "string.quoted.double.heredoc.powershell",
                    "settings": {
                        "foreground": "#72C09F"
                    }
                },
                {
                    "scope": "string.quoted.single.heredoc.powershell",
                    "settings": {
                        "foreground": "#72C09F"
                    }
                },
                {
                    "scope": "string.quoted.single.powershell",
                    "settings": {
                        "foreground": "#72C09F"
                    }
                },
                {
                    "scope": "keyword.control.powershell",
                    "settings": {
                        "foreground": "#A87084",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "comment.line.powershell",
                    "settings": {
                        "foreground": "#5E7887",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "comment.line",
                    "settings": {
                        "foreground": "#4a8a4a",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "meta.scriptblock.powershell",
                    "settings": {
                        "foreground": "#BE856F",
                    }
                },
                {
                    "scope": "keyword.operator.comparison.powershell",
                    "settings": {
                        "foreground": "#A87084",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "keyword.operator.assignment.powershell",
                    "settings": {
                        "foreground": "#A87084",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "source.powershell",
                    "settings": {
                        "foreground": "#BE856F"
                    }
                }
            ],
        },
    },
