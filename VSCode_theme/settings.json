  "todo-tree.highlights.customHighlight": {
      "TODO": {
          "icon": "$(gist)",
          "type": "text",
          "foreground": "gray",
          "background": "blue",
          "opacity": 50
      },
      "[ ]": {
          "icon": "$(list-unordered)",
          "type": "text",
          "foreground": "gray",
          "background": "blue",
          "opacity": 50
      },
      "HiW": {
          "icon": "$(diff-added)",
          "type": "text",
          "foreground": "black",
          "background": "orange",
          "opacity": 50
      },
      "WTF": {
          "icon": "$(diff-added)",
          "type": "text",
          "foreground": "gray",
          "background": "orange",
          "opacity": 50
      },
      "BUG": {
          "icon": "$(bug)",
          "type": "text",
          "foreground": "black",
          "background": "yellow"
      },
      "FIX": {
          "icon": "$(flame)",
          "type": "text",
          "foreground": "black",
          "background": "red"
      },
      "ADDs": {
          "icon": "$(diff-added)",
          "type": "text",
          "foreground": "black",
          "background": "green"
      }
  },
